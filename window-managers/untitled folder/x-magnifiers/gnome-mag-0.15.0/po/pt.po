#: ../colorblind/ui/About.py:36
msgid "translator-credits"
msgstr "Duarte Loreto <happyguy_pt@hotmail.com>"

#: ../colorblind/ui/ColorblindPreferencesUI.py:105
msgid "New accelerator..."
msgstr "Nova tecla de atalho..."

#: ../colorblind/ui/ColorblindPreferencesUI.py:157
#, python-format
msgid ""
"The shortcut \"%s\" cannot be used because it will become impossible to type "
"using this key.\n"
"Please try with a key such as Control, Alt or Shift at the same time.\n"
msgstr ""
"O atalho \"%s\" não pode ser utilizado porque tornaria impossível escrever "
"com esta tecla.\n"
"Tente com uma tecla tal como o Control, Alt ou Shift simultaneamente.\n"

#: ../magnifier/GNOME_Magnifier.server.in.in.h:1
msgid "Magnifier"
msgstr "Lupa"

#: ../magnifier/GNOME_Magnifier.server.in.in.h:2
msgid "Simple Screen Magnification Service"
msgstr "Simples Serviço de Ampliação de Ecrã"

#: ../magnifier/magnifier-main.c:408
#, c-format
msgid "Can't open display: DISPLAY is not set"
msgstr "Incapaz de abrir o ecrã: DISPLAY não está definida"
