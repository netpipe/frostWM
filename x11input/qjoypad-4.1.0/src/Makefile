	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_getkey.o moc_getkey.cpp

moc_layout_edit.o: moc_layout_edit.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_layout_edit.o moc_layout_edit.cpp

####### Install

install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)//usr/local//bin/ || $(MKDIR) $(INSTALL_ROOT)//usr/local//bin/ 
	-$(INSTALL_PROGRAM) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"
	-$(STRIP) "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)//usr/local//bin/ 


install_icons: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)//usr/local//share/pixmaps/qjoypad/ || $(MKDIR) $(INSTALL_ROOT)//usr/local//share/pixmaps/qjoypad/ 
	cp ../icons/* //usr/local//share/pixmaps/qjoypad; cd //usr/local//share/pixmaps/qjoypad; ln -sf gamepad4-24x24.png icon24.png; ln -sf gamepad3-64x64.png icon64.png; chmod -R a+r //usr/local//share/pixmaps/qjoypad


install_doc: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)//usr/local//share/doc/qjoypad4/ || $(MKDIR) $(INSTALL_ROOT)//usr/local//share/doc/qjoypad4/ 
	cp ../README.txt ../LICENSE.txt //usr/local//share/doc/qjoypad4


install:  install_target install_icons install_doc  FORCE

uninstall: uninstall_target   FORCE

FORCE:

